  object query_Pagamentos_Pendentes: TFDQuery
    Connection = fd
    SQL.Strings = (
      ''
      '    SELECT'
      '      s.idAgendamento,'
      '      SUM(s.preco - s.desconto) AS Total_Devido, '
      '      COALESCE(SUM(p.valor_pago), 0) AS Total_Pago,'
      
        '      (SUM(s.preco - s.desconto) - COALESCE(SUM(p.valor_pago), 0' +
        ')) AS Valor_Pendente'
      '    FROM agendamento_servicos s '
      
        '    LEFT JOIN agendamento_pagamento p ON s.idAgendamento = p.idA' +
        'gendamento'
      '    GROUP BY s.idAgendamento '
      '    HAVING Valor_Pendente > 0;'
      '')
    Left = 386
    Top = 640
  end
  object DsPagamentosPendentes: TDataSource
    DataSet = query_Pagamentos_Pendentes
    Left = 547
    Top = 640
  end
  object DsMetodosPagamento: TDataSource
